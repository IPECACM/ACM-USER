                        android:layout_height="20sp"
                        android:text="Batch"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="normal" />

                    <TextView
                        android:layout_width="100sp"
                        android:layout_height="20sp"
                        android:text="2018-2022"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="normal" />

                    <TextView
                        android:layout_width="100sp"
                        android:layout_height="20sp"
                        android:text="CSE"
                        android:textColor="@color/black"
                        android:textSize="18sp"
                        android:textStyle="normal" />

                </androidx.appcompat.widget.LinearLayoutCompat>


